#!/usr/bin/env php
<?php

require_once 'bootstrap.php';

use Symfony\Component\Console\Application;

$application = new Application();

$application->add(new \Kiabi\Command\FeedDownloadCommand());
$application->add(new \Kiabi\Command\YandexParseCommand());
$application->add(new \Kiabi\Command\YandexCityParseCommand());
$application->add(new \Kiabi\Command\MailParseCommand());
$application->add(new \Kiabi\Command\GoogleParseCommand());
$application->add(new \Kiabi\Command\Google2ParseCommand());
$application->add(new \Kiabi\Command\FacebookParseCommand());
$application->add(new \Kiabi\Command\CriteoParseCommand());
$application->add(new \Kiabi\Command\AvitoParseCommand());
$application->add(new \Kiabi\Command\RBFParseCommand());
$application->add(new \Kiabi\Command\RBF2ParseCommand());
$application->add(new \Kiabi\Command\RBFConvertCommand());
$application->add(new \Kiabi\Command\RBFCheckCommand());
$application->add(new \Kiabi\Command\GoogleCategoryParseCommand());
$application->add(new \Kiabi\Command\GoogleCategoryCompleteCommand());
$application->add(new \Kiabi\Command\GoogleTaxonomyParseCommand());
$application->add(new \Kiabi\Command\YandexCategoryParseCommand());
$application->add(new \Kiabi\Command\YandexColorParseCommand());
$application->add(new \Kiabi\Command\YandexColorCleanCommand());
$application->add(new \Kiabi\Command\VKParseCommand());

$application->run();